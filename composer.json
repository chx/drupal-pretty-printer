{
    "name": "chx/drupal-pretty-printer",
    "description": "Drupal pretty printer",
    "license": "GPL-2.0-or-later",
    "require": {
        "nikic/php-parser": "^4.18 || ^5"
    },
    "require-dev": {
        "drupal/coder": "^8.3.10"
    },
    "scripts": {
        "phpcs": "phpcs --standard=phpcs.xml.dist --"
    },
    "autoload": {
        "psr-4": {
            "DrupalPrettyPrinter\\": "src"
        }
    },
    "authors": [
        {
            "name": "Fran Garcia (fjgarlin)",
            "role": "Developer"
        },
        {
            "name": "Karoly Negyesi (chx)",
            "role": "Maintainer"
        }
    ],
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
